cyoAdventure = function (a) {
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
    /// Reagan Nominee now
    if ((campaignTrail_temp.player_answers[0] == 12794)) {
        campaignTrail_temp.candidate_json[0]["fields"].first_name = "Ronald";
        campaignTrail_temp.candidate_json[0]["fields"].last_name = "Reagan";
        campaignTrail_temp.question_number = 29;
    e.ElectionPopup = "It's been a rocky campaign season, which you were not apart of. Let's see how Reagan does."
    e.WinPopup = "Damn. Didn't think he'd pull it off, huh?"
    e.LosePopup = "To be expected."
    }
}

